                                            div.skill Ternura #[div.skill-p ]
                                            div.skill Hermosura #[div.skill-p ]
                                            div.skill Agrevisidad #[div.skill-p ]
                                            div.skill Valent√≠a #[div.skill-p ]
                                        
                                        div.eyes
