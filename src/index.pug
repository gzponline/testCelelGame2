                                                div.ear__left.face 
                                                div.ear__top.face 
                                                div.ear__bottom.face 
                                    div.head__bottom.face 
                            div.neck__bottom.face  
                            
                    div.body__bottom.face 
                
                div.paws
                    - var n = 0
                        while n < 4
                            div.paw-b.face
                                div.paw-b__front.face 
                                div.paw-b__back.face 
                                div.paw-b__right.face 
                                div.paw-b__left.face 
                                div.paw-b__top.face 
                                div.paw-b__bottom.face 
                            - n++
                div.paws
                    - var n = 0
                        while n < 4
                            div.paw-f.face
                                div.paw-f__front.face 
                                div.paw-f__back.face 
                                div.paw-f__right.face 
                                div.paw-f__left.face 
                                div.paw-f__top.face 
                                div.paw-f__bottom.face 
                            - n++

    
