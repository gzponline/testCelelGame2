        link(rel="stylesheet", type="text/css", href="main.css")
    body
    
        div.main#m
            div.btns
                a()#btn-0.btn
                a()#btn-1.btn
                a()#btn-2.btn
                
            div.dog#d
                div.body.face
                    div.body__front.face 
                    div.body__back.face 
                    div.body__right.face 
                    div.body__left.face 
                    div.body__top.face 
                    
                        div.tail.face
